{
    "ssl": {
        "trustStorePath": "ssl/client/server-truststore.jks",
        "trustStorePassword": "password",
        "keyStorePath": "ssl/server/server-keystore.jks",
        "keyStorePassword": "password",
        "symKeyPath": "ssl/server/symKey.key",
        "proxyHttpsPort": 8989
    },
    "assets":["ico", "png", "jpg", "jpeg", "gif", "css", "js", "txt"],
    "defaultService" : "sb",
    "maxPayloadSizeBytes": "512k",
    "rewriteRules": {
        "sb": {
            "protocol": "http",
            "host": "localhost",
            "port": 8080
        },
        "google": {
            "protocol": "http",
            "host": "google.com",
            "port": 80
        },
        "acl": {
            "protocol": "http",
            "host": "localhost",
            "port": 9001
        },
        "um": {
            "protocol": "http",
            "host": "localhost",
            "port": 9000
        }
    }

}